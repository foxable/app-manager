    "react-router": "3.*"
  },
  "devDependencies": {
    "@types/cheerio": "0.*",
    "@types/classnames": "0.*",
    "@types/core-js": "0.*",
    "@types/electron": "1.*",
    "@types/node": "7.*",
    "@types/react": "15.*",
    "@types/react-dom": "0.*",
    "@types/react-router": "4.*",
    "core-js": "2.*",
    "electron": "1.6.*",
    "source-map-loader": "0.2.*",
    "ts-loader": "2.*"
  }
